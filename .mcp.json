{
  "mcpServers": {
    "mongodb": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "mcp-mongo-server"
      ],
      "env": {
        "MCP_MONGODB_URI": "${MONGO_URI}",
        "MCP_MONGODB_READONLY": "true"
      }
    },
    "jetbrains": {
      "type": "sse",
      "url": "http://localhost:64343/sse"
    }
  }
}